{
  "trailingComma": "none",
  "printWidth": 100,
  "jsxSingleQuote": true,
  "singleQuote": true,
  "tabWidth": 2,
  "semi": true,
  "arrowParens": "always",
  "bracketSpacing": true,
  "bracketSameLine": false,

  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/pages/(.*$)",
    "^@/components/(.*$)",
    "^@/providers/(.*$)",
    "^@/api/(.*$)",
    "^@/store/(.*$)",
    "^@/types/(.*$)",
    "^@/hooks/(.*$)",
    "^@/styles/(.*$)",
    "^[./](.*$)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}